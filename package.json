{
  "name": "tarkov-map-viewer",
  "version": "1.0.0",
  "description": "view tarkov maps one click",
  "main": "index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron ./index",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aar0nsky/tarkov-map-viewer.git"
  },
  "author": "Aaron Ankiel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aar0nsky/tarkov-map-viewer/issues"
  },
  "homepage": "https://github.com/aar0nsky/tarkov-map-viewer#readme",
  "devDependencies": {
    "@typescript-eslint/parser": "^5.19.0",
    "electron": "18.0.3",
    "electron-builder": "^23.0.3",
    "electron-reloader": "^1.2.3",
    "eslint": "^8.13.0",
    "prettier": "^2.6.2",
    "prettier-eslint": "^13.0.0"
  },
  "dependencies": {
    "electron-default-menu": "^1.0.2",
    "material-design-lite": "^1.3.0"
  },
  "build": {
    "appId": "Tarkov Toolkit",
    "extraResources": 
      [
        {
          "from": "config.json",
          "to": ".",
          "filter": ["**/*"]
        }
      ]
    ,
    "win": {
      "target": "NSIS"
    }
  }
}
